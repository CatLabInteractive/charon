{
  "name": "catlabinteractive/charon",
  "description": "Charon is a framework for building self documented RESTful API's.",
  "type": "library",
  "require": {
    "php": ">=7.4",
    "catlabinteractive/requirements": "^1.0.5",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*",
    "catlabinteractive/cursor-pagination" : "^1.0.7"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Thijs Van der Schaeghe",
      "email": "info@catlab.be"
    }
  ],
  "autoload": {
    "psr-4": {
      "CatLab\\Charon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/",
      "App\\":"example/"
    }
  },
  "minimum-stability" : "dev"
}
