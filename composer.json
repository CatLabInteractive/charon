{
  "name": "catlabinteractive/charon",
  "description": "Charon is a framework for building self documented RESTfull API's.",
  "type": "library",
  "require": {
    "php": ">=7.0.0",
    "catlabinteractive/requirements": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.3.*",
    "catlabinteractive/cursor-pagination" : "~1.0.4"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Thijs Van der Schaeghe",
      "email": "info@catlab.be"
    }
  ],
  "autoload": {
    "psr-4": {
      "CatLab\\Charon\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Petstore\\": "tests/Petstore",
      "App\\":"example/"
    }
  },
  "minimum-stability" : "dev"
}
